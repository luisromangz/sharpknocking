    <readmeFile></readmeFile>
    <licenseFile></licenseFile>
    <wrapLicenseFileText>1</wrapLicenseFileText>
    <leftImage></leftImage>
    <logoImage>/home/luis/programacion/csl-csknocking-g/Common/Artwork/sharpknocking48.png</logoImage>
    <splashImage></splashImage>
    <defaultLanguage>es</defaultLanguage>
